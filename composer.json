{
    "name": "aligent/oro-fixtures",
    "description": "An OroCommerce Bundle to use Alice fixtures to populate development environments with test data.",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Aligent\\FixturesBundle\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Adam Hall",
            "email": "adam.hall@aligent.com.au"
        }
    ],
    "repositories": {
        "oro": {
            "type": "composer",
            "url": "https://packagist.orocrm.com"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "oro/commerce": "4.2.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "phpstan/phpstan": "^1.7",
        "phpmd/phpmd": "^2.12",
        "friendsofphp/php-cs-fixer": "~2.18.2",
        "nelmio/alice": "~3.8.0",
        "theofidry/alice-data-fixtures": "~1.4.0",
        "symfony/phpunit-bridge": "~4.4.24",
        "squizlabs/php_codesniffer": "^3.6"
    }
}
